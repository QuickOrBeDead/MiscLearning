{
  "RabbitMqConnection": {
    "Host": "host.docker.internal",
    "UserName": "guest",
    "Password": "guest"
  },
  "RabbitMqConsumer": {
    "Queue": "OrderMail",
    "Exchanges": [
      "Order.OrderCreated"
    ],
    "SingleActiveConsumer": false,
    "TryCount": 3,
    "RetryPolicySleepDurationInMs": 5000
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}
